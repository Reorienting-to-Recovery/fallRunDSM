Package: fallRunDSM
Type: Package
Title: Structured Decision Fall Run Model for Reorienting to Recovery
Version: 2.0
Authors@R: c(
    person("James", "Petterson", email = "jt.peterson@oregonstate.edu", role = "aut"),
